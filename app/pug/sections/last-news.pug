include ../data/news

include ../mixins/photo-list
include ../mixins/pagination

section.last-news
  .last-news__container.container
    h2.section-title.last-news__header Последние новости
    .last-news__box
      +photo-list({
        list: news,
        blockClass: 'last-news__photo-list'
      })
    +pagination({
      blockClass: 'last-news__pagination',
      pagesCount: 7
    })